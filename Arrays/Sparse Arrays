#include<bits/stdc++.h>
using namespace std;

int main()
{
   int n, q;
   map <string, int> m;                     // map function to keep track of counts of integers
   string s;

    cin>>n;
    for(int i=0; i<n; i++)
        {
            cin>>s;
            m[s]++;
        }

    cin>>q;
    for(int i=0; i<q; i++)
    {
        cin>>s;
        cout<<m[s]<<endl;
    }
    return 0;
}

